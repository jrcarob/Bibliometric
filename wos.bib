@article{ WOS:000304336800052,
Author = {Raudenbush, Bryan and Capiola, August},
Title = {Physiological responses of food neophobics and food neophilics to food
   and non-food stimuli},
Journal = {APPETITE},
Year = {2012},
Volume = {58},
Number = {3},
Pages = {1106-1108},
Month = {JUN},
Abstract = {Individual differences in human food neophobia (the reluctance to try
   novel foods) and food neophilia (the overt willingness to try novel
   foods) influence the evaluation of tastes and odors, as well as the
   sampling of such stimuli. Past research also notes an association of
   food neophobia to PTC sensitivity, body weight, and cephalic phase
   salivary response. The present study assessed physiological reactions of
   food neophobics and neophilics to pictures of food and non-food stimuli.
   Stimuli pictures were presented in random order on a computer screen for
   a period of 5 min. No significant differences were found between the
   groups in relation to non-food stimuli. However, pulse, GSR, and
   respirations were significantly increased in food neophobics when
