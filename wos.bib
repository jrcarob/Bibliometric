@article{ WOS:000733237500001,
Author = {Khanna, Suvidha and Nagar, Komal and Chauhan, Vinay and Bhagat, Sheetal},
Title = {Application of the extended theory of planned behavior to street-food
   consumption: testing the effect of food neophobia among Indian consumers},
Journal = {BRITISH FOOD JOURNAL},
