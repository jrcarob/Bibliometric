@article{ WOS:000565604900002,
Author = {Camacho-Alpizar, Andres and Griffin, Andrea S. and Guillette, Lauren M.},
Title = {Are cognitive abilities under selection by female choice? A comment on
   Chen et al. (2019)},
Journal = {ANIMAL BEHAVIOUR},
Year = {2020},
Volume = {165},
Pages = {E1-E3},
Month = {JUL},
Abstract = {A recent paper by Chen et al. (2019, Science, 363 (6423), 166-167) found
   that female budgerigars, Melopsittacus undulatus, show a preference for
   males that are successful problem solvers. Is this result sufficient to
   conclude that mate choice influences the evolution of cognitive
   abilities? Here, we consider what we currently know about the
   relationship between problem solving and cognition. We argue that it is
   premature to conclude, based on these findings, that cognitive abilities
   have evolved in part through sexual selection by female choice. (c) 2020
   The Association for the Study of Animal Behaviour. Published by Elsevier
   Ltd. All rights reserved.},
