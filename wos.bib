@article{ WOS:000625353200011,
Author = {Milijevic, Srdjan and Sklavounos, Nikolaos and Rotsios, Konstantinos},
Title = {CONSUMERS' PURCHASE INTENTIONS TOWARDS NOVEL DAIRY PRODUCTS: EVIDENCE
   FROM GREECE AND SERBIA},
Journal = {STUDIES IN BUSINESS AND ECONOMICS},
