Author = {Jarjour, Catherine and Evans, Julian C. and Routh, Melanie and
   Morand-Ferron, Julie},
Title = {Does city life reduce neophobia? A study on wild black-capped chickadees},
Journal = {BEHAVIORAL ECOLOGY},
