@article{ WOS:000399568000004,
Author = {Dimitrovski, Darko and Crespi-Vallbona, Montserrat},
Title = {Role of food neophilia in food market tourists' motivational construct:
   The case of La Boqueria in Barcelona, Spain},
Journal = {JOURNAL OF TRAVEL \& TOURISM MARKETING},
Year = {2017},
Volume = {34},
Number = {4},
Pages = {475-487},
Abstract = {In this paper we advocate the study of local food markets to explore
   fundamental issues about the relationship between these urban tourist
   attractions and visitors' motivation and satisfaction. Based on a survey
   in La Boqueria Market in Barcelona, Spain, we test the direct effect of
   escape from routine, cultural experience, prestige, and food market
   involvement on satisfaction, and also the moderator role of food
   neophilia in this relationship. We conclude that tourists visiting food
   markets are motivated mainly by cultural experience and interaction with
   local producers, while food neophilia plays a significant moderator
   role.},
